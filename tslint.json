{
  "defaultSeverity": "error",
  "extends": [
    "plugin:vue/essential",
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "indent": [
      true,
      "tabs",
      2
    ],
    "no-any": true,
    "quotemark": [
      true,
      "single"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ]
  }
}